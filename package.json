{
  "name": "cloudmusic",
  "displayName": "cloudmusic",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cloudmusic",
          "title": "cloudmusic",
          "icon": "media/disc.svg"
        }
      ]
    },
    "views": {
      "cloudmusic": [
        {
          "id": "account",
          "name": "account"
        },
        {
          "id": "playlist",
          "name": "playlist"
        },
        {
          "id": "queue",
          "name": "queue"
        }
      ]
    },
    "commands": [
      {
        "command": "cloudmusic.refreshPlaylist",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh-ccw.svg",
          "dark": "resources/dark/refresh-ccw.svg"
        }
      },
      {
        "command": "cloudmusic.refreshPlaylistContent",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh-ccw.svg",
          "dark": "resources/dark/refresh-ccw.svg"
        }
      },
      {
        "command": "cloudmusic.playPlaylist",
        "title": "Play",
        "icon": {
          "light": "resources/light/play-circle.svg",
          "dark": "resources/dark/play-circle.svg"
        }
      },
      {
        "command": "cloudmusic.addPlaylist",
        "title": "Add",
        "icon": {
          "light": "resources/light/plus-circle.svg",
          "dark": "resources/dark/plus-circle.svg"
        }
      },
      {
        "command": "cloudmusic.clearQueue",
        "title": "Clear",
        "icon": {
          "light": "resources/light/trash-2.svg",
          "dark": "resources/dark/trash-2.svg"
        }
      },
      {
        "command": "cloudmusic.randomQueue",
        "title": "Shuffle",
        "icon": {
          "light": "resources/light/shuffle.svg",
          "dark": "resources/dark/shuffle.svg"
        }
      },
      {
        "command": "cloudmusic.addSong",
        "title": "Add",
        "icon": {
          "light": "resources/light/plus-circle.svg",
          "dark": "resources/dark/plus-circle.svg"
        }
      },
      {
        "command": "cloudmusic.playSongWithPlaylist",
        "title": "Play",
        "icon": {
          "light": "resources/light/play-circle.svg",
          "dark": "resources/dark/play-circle.svg"
        }
      },
      {
        "command": "cloudmusic.playSong",
        "title": "Play",
        "icon": {
          "light": "resources/light/play-circle.svg",
          "dark": "resources/dark/play-circle.svg"
        }
      },
      {
        "command": "cloudmusic.deleteSong",
        "title": "Play",
        "icon": {
          "light": "resources/light/x-circle.svg",
          "dark": "resources/dark/x-circle.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "cloudmusic.refreshPlaylist",
          "when": "view == playlist",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.clearQueue",
          "when": "view == queue",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.randomQueue",
          "when": "view == queue",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "cloudmusic.refreshPlaylistContent",
          "when": "view == playlist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playPlaylist",
          "when": "view == playlist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.addPlaylist",
          "when": "view == playlist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.addSong",
          "when": "view == playlist && viewItem == PlaylistContentTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playSongWithPlaylist",
          "when": "view == playlist && viewItem == PlaylistContentTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playSong",
          "when": "view == queue && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.deleteSong",
          "when": "view == queue && viewItem == QueueItemTreeItem",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "NeteaseCloudMusicApi": "^3.34.0",
    "array-unsort": "^1.1.5"
  }
}
